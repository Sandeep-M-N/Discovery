# GitHub Configuration
GITHUB_TOKEN=your_github_token_here

# GitLab Configuration  
GITLAB_TOKEN=your_gitlab_personal_access_token_here
GITLAB_URL=https://jitlocker.changepond.com

# Note: For jitlocker.changepond.com, you need:
# 1. A personal access token with 'read_repository' scope
# 2. Access to the specific repository
# 3. The token should be created from: https://jitlocker.changepond.com/-/profile/personal_access_tokens

# Azure OpenAI Configuration
AZURE_OPENAI_API_KEY=your_azure_openai_key
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com
AZURE_OPENAI_DEPLOYMENT=gpt-4o-mini
OPENAI_API_VERSION=2024-02-15-preview

# Langfuse Configuration (optional)
LANGFUSE_SECRET_KEY=your_langfuse_secret_key
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key
LANGFUSE_HOST=https://cloud.langfuse.com